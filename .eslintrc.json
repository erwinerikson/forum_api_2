{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-trailing-spaces": "off",
        "class-methods-use-this": "off",
        "no-unused-expressions": "off",
        "no-param-reassign": "off",
        "camelcase": "off"
    }
}
